						%li<
							%a(href="#")<
								%img(src="#{site.jborg_images_url}/common/socialmedia_icon16_linkedin.png")
						%li<
							%a(href="#")<
								%img(src="#{site.jborg_images_url}/common/socialmedia_icon16_twitter.png")
						%li<
							%a(href="#")<
								%img(src="#{site.jborg_images_url}/common/socialmedia_icon16_facebook.png")
						%li<
							%a(href="#")<

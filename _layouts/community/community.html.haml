							%ul.nav.pull-right
								%li
									%form.navbar-search
										%input.search-query#communitysearch(type="text" placeholder="Search")
								%li.divider-vertical
								%li
									.btn-group
										%a.btn.btn-info.btn-small.dropdown-toggle(data-toggle="dropdown" href="#LogInRegister")
											= succeed ' Log In | Register' do
												%i.icon-user
											%span.caret
										%ul.dropdown-menu
											%li<
												%a(href="#") Log In
											%li<
												%a(href="#") Register
											%li.divider
											%li<
												%a(href="#") Forgot Username
											%li<
												%a(href="#") Forgot Password

			~ content

		.company
			%a(href="http://www.redhat.com/")<
				%img(src="#{site.jborg_images_url}/common/redhat_logo.png")
				
		%script(src="#{site.jborg_js_url}/libs/jquery/1.9.1/jquery#{site.minified}.js")

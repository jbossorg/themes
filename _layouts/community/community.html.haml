!!! 5
%html(lang="en")
	%head
		%meta(charset="utf-8")
		%meta(name="viewport" content="width=device-width, initial-scale=1.0")
		%meta(name="description" content="#{page.description}")
		%meta(name="author" content="#{page.author}")
		%title #{page.title} Â· #{site.title}

		- pageStyle = page.style ;
		%link(href="#{pageStyle ? site[pageStyle].bootstrap_css_url : site.bootstrap_css_url}#{site.minified}.css" rel="stylesheet" media="screen")

		/ IE 6-8 support of HTML 5 elements
		/[if lt IE 9]
			%script(src="#{site.jborg_js_url}/libs/html5/pre3.6/html5.min.js")

		%link(rel="shortcut icon" href="#{site.jborg_images_url}/common/favicon.ico")
		%link(rel="apple-touch-icon-precomposed" sizes="144x144" href="#{site.jborg_images_url}/community/jboss-touch-icon-144-precomposed.png")
		%link(rel="apple-touch-icon-precomposed" sizes="114x114" href="#{site.jborg_images_url}/community/jboss-touch-icon-114-precomposed.png")
		%link(rel="apple-touch-icon-precomposed" sizes="72x72" href="#{site.jborg_images_url}/community/jboss-touch-icon-72-precomposed.png")
		%link(rel="apple-touch-icon-precomposed" href="#{site.jborgimages_url}/community/jboss-touch-icon-57-precomposed.png")

	%body
		.container
			.globalbanner
				.socialmedia
					%ul
						%li<
							%a(href="#")<
								%img(src="#{site.jborg_images_url}/common/socialmedia_icon16_asylum.png")
						%li<
							%a(href="#")<
								%img(src="#{site.jborg_images_url}/common/socialmedia_icon16_planet.png")
						%li<

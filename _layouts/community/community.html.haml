						%li<
							%a(href="#")<
								%img(src="#{site.jborg_images_url}/common/socialmedia_icon16_email.png")
				.globalbrand
					%a(href="http://www.jboss.org")<
						%img(src="#{site.jborg_images_url}/common/jbosscommunitylogo_large.png")
			.navbar#navbar-fix
				.navbar-inner
					.container
						%a.btn.btn-navbar(data-toggle="collapse" data-target=".nav-collapse" style="margin-top: 2px;") menu
						.nav-collapse.collapse
							%ul.nav
								%li.active<
									%a(href="#") Home
								%li.dropdown
									%a.dropdown-toggle(data-toggle="dropdown" href="#Developers")<
										= precede 'Developers ' do
											%b.caret>
									%ul.dropdown-menu
